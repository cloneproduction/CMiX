<UserControl x:Class="CMiX.Views.LayerFX"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Controls="clr-namespace:CMiX.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="80.667" d:DesignWidth="202.667">
    
    <StackPanel>
        <Controls:CustomSlider
            Height="20"
            Margin="0, 0, 0, 2"
            Caption="Feedback"
            Value="{Binding Feedback, Mode=TwoWay}"
            />
        <Controls:CustomSlider
            Height="20"
            Margin="0"
            Caption="Blur"
            Value="{Binding Blur, Mode=TwoWay}"
            />
    </StackPanel>
</UserControl>