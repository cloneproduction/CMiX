<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    
    <FontFamily x:Key="DefaultFont">Microsoft Sans Serif</FontFamily>
    <system:Double x:Key="DefaultFontSize">11</system:Double>

    <Color x:Key="HighDarker">#FF1e1e1e</Color>
    <Color x:Key="Darker">#FF282828</Color>
    
    <Color x:Key="Dark">#FF323232</Color>
    <Color x:Key="Normal">#FF404040</Color>
    <Color x:Key="Light">#FF5A5A5A</Color>
    <Color x:Key="HighLight">#FF787878</Color>

    <Color x:Key="Over">#FF4D4D4D</Color>
    <Color x:Key="Active">#FF7D00FF</Color>

    <Color x:Key="Text">#FFB4B4B4</Color>
    <Color x:Key="OverText">Gainsboro</Color>
    <Color x:Key="ActiveText">White</Color>
</ResourceDictionary>