<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Colors.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="SuperHighDarkerColor" Color="{StaticResource SuperHighDarker}" />
    <SolidColorBrush x:Key="HighDarkerColor" Color="{StaticResource HighDarker}" />
    <SolidColorBrush x:Key="DarkerColor" Color="{StaticResource Darker}" />
    <SolidColorBrush x:Key="DarkColor" Color="{StaticResource Dark}" />
    <SolidColorBrush x:Key="Color" Color="{StaticResource Normal}" />
    <SolidColorBrush x:Key="LightColor" Color="{StaticResource Light}" />
    <SolidColorBrush x:Key="HighLightColor" Color="{StaticResource HighLight}" />
    <SolidColorBrush x:Key="SuperHighLightColor" Color="{StaticResource SuperHighLight}" />
    
    <SolidColorBrush x:Key="OverColor" Color="{StaticResource Over}" />
    <SolidColorBrush x:Key="ActiveColor" Color="{StaticResource Active}" />
    <SolidColorBrush x:Key="TextBase" Color="{StaticResource Text}" />
    <SolidColorBrush x:Key="OverTextBase" Color="{StaticResource OverText}" />
    <SolidColorBrush x:Key="ActiveTextBase" Color="{StaticResource ActiveText}" />
</ResourceDictionary>